<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
        "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
    <class name="com.nestor.logic.Sessions" table="sys.sessions">
        <id column="session_id" name="id" type="java.lang.Integer">
            <generator class="increment"/>
        </id>
        <property column="publicAddress" name="publicAddress" type="java.lang.String"/>
        <property column="serverId" name="serverId" type="java.lang.String"/>
        <property column="nodeName" name="nodeName" type="java.lang.String"/>
        <property column="createdTime" name="createdTime" type="java.lang.Long"/>
        <property column="deleteTime" name="deleteTime" type="java.lang.Long"/>
    </class>
</hibernate-mapping>